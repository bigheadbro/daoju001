<div class="reg-win">
    <div class="reg-layer">
        <h3>
            <a class="closealert" href="#">×</a>注册
        </h3>
        <div class="reg-con clearfix">
            <div class="left">
                <h2>
                    <a class="fr" href="#">登录</a>快速注册
                </h2>
                <form id="reg_form" method="post" action="">
                    <!-- 隐藏字段，用于标示是表单提交 -->
                    <input type="hidden" name="do_submit" value="true"/>
                    #springBind("form.name")
                    <div class="reg_seg">
                        <input type="text" value="$!{status.value}" name="$!{status.expression}" placeholder="请输入用户名"/>
                    </div>
                    #springBind("form.email")
                    <div class="reg_seg">
                        <input name="$!{status.expression}" type="text" placeholder="请输入邮箱"/>
                    </div>
                    #springBind("form.pwd")
                    <div class="reg_seg">
                        <input id="password" name="$!{status.expression}" type="password" placeholder="请输入密码"/>
                    </div>
                </form>
                <a class="submit_reg" href="">注册</a>
                <script>
                    $(function() {
                      $("#submit_reg").click(function () {
                        $("#reg_form").submit();//提交
                      });
                    });
                </script>
            </div>
        </div>
    </div>
</div>