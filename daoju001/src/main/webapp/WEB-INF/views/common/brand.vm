<div id="pagemask-brand"></div>
<div class="brand-win">
    <div class="brand-win-content">
        <h2>
            <a class="close-brand">x</a>
				选择品牌(按拼音首字母排序)
		</h2>
        <div class="content clearfix">
			#foreach($brand in $brands)
				<a class="brand-item" key="${brand.key}">
					<div class="brand-logo">
						<span>
							<img src="/img/logo/${brand.link}" />
						</span>
					</div>
					<span class="name">${brand.name}</span>
				</a>
			#end
        </div>
    </div>
</div>
